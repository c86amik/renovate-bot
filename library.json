{
  "extends": [
    "config:base",
    ":labels(dependencies,CVE)",
    ":separateMajorReleases",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":prImmediately",
    ":semanticPrefixFixDepsChoreOthers",
    ":updateNotScheduled",
    ":automergeDisabled",
    ":ignoreModulesAndTests",
    ":autodetectPinVersions",
    ":prHourlyLimit2",
    ":prConcurrentLimit20",
    "group:monorepos",
    "group:recommended",
    "workarounds:all"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "org.slf4j.*","com.cerner.maven:maven-base-pom"
      ],
      "enabled": false
    },
    {
      "matchPackageNames": ["jar"],
      "enabled": false
    },
    {
      "matchDepTypes": [
        "devDependencies",
        "peerDependencies"
      ],
      "enabled": false
    }
  ],
  "baseBranches": ["master"],
  "separateMajorMinor": false,
  "lockFileMaintenance": {
    "enabled": false
  },
  "branchName": "renovate-stay-current",
  "commitBodyTable": true,
  "commitMessageAction": "Auto-update",
  "commitMessageExtra": "cerner dependencies",
  "prTitle": "Update dependencies by Rennovate Bot."
}